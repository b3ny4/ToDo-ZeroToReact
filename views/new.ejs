<% layout('layout/boilerplate') %>
    <h1>New ToDo</h1>
    <form action="/" method="post">
        <div class="mb-3">
            <label for="todo" class="form-label">ToDo</label>
            <input type="text" class="form-control" id="todo" name="todo">
        </div>
        <button type="submit" class="btn btn-primary">Save</button>
    </form>